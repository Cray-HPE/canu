{# storage #}
***===> ncn-s.lag.j2
{%- for node in cabling.nodes %}
{%- if node.subtype == "storage" %}

interface lag ***===> {{ node.config.LAG_NUMBER }} multi-chassis
    no shutdown
    description ***===> {{ node.config.DESCRIPTION }}
    no routing
    vlan trunk native 1
    vlan trunk allowed 1-2,4,7,10
    lacp mode active
    lacp fallback
    spanning-tree port-type admin-edge

interface ***===> {{ node.config.INTERFACE_LAG }}
    no shutdown
    mtu 9198
    description ***===> {{ node.config.DESCRIPTION }}    
    lag ***===> {{ node.config.LAG_NUMBER }}
{%- endif %}
{%- endfor %}
***===> end ncn-s.lag.j2
{#- end storage #}