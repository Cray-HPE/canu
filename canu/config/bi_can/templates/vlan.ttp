<group name = "vlan.{{ vlan }}">
vlan {{ vlan }}
</group>
<group name = "vlan.{{ vlan }}**">
interface vlan {{ vlan }}
    vrf attach {{ vrf }}
    description {{ interface_description }}
    ip mtu {{ mtu }}
    ip address {{ ip | is_ip }}
    active-gateway ip mac {{ active_gateway_ip_mac }}
    active-gateway ip {{ active_gateway_ip | is_ip }}
    ip ospf {{ ospf_instance }} area {{ area }}
</group>